{
  "author": "Drake Bott",
  "license": "MIT",
  "homepage": "https://github.com/bottd/vite-plugin-norg#README",
  "name": "vite-plugin-norg",
  "version": "2.2.0",
  "type": "module",
  "description": "A Vite plugin for rendering Norg files",
  "main": "dist/plugin/index.js",
  "types": "dist/plugin/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    },
    "./svelte": {
      "types": "./dist/plugin/generators/svelte.d.ts"
    },
    "./react": {
      "types": "./dist/plugin/generators/react.d.ts"
    },
    "./html": {
      "types": "./dist/plugin/generators/html.d.ts"
    },
    "./metadata": {
      "types": "./dist/plugin/generators/metadata.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:napi && npm run build:js",
    "build:napi": "napi build --release --platform --esm --output-dir dist/napi/",
    "build:js": "vite build",
    "clean": "rimraf dist target",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "test": "bun test"
  },
  "keywords": [
    "vite",
    "plugin",
    "norg",
    "neorg",
    "wasm",
    "napi",
    "rust",
    "svelte",
    "react"
  ],
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/npm": "13.1.4",
    "@semantic-release/git": "10.0.1",
    "ajv": "8.17.1",
    "@napi-rs/cli": "3.5.1",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "25.2.2",
    "@types/react": "19.2.13",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "jiti": "2.6.1",
    "prettier": "3.8.1",
    "react": "19.2.4",
    "rimraf": "6.1.2",
    "svelte": "5.50.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "semantic-release": "25.0.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vite-tsconfig-paths": "6.1.0",
    "zod": "4.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bottd/vite-plugin-norg.git"
  },
  "dependencies": {
    "ts-dedent": "^2.2.0"
  }
}
