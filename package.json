{
  "author": "Drake Bott",
  "license": "MIT",
  "homepage": "https://github.com/bottd/vite-plugin-norg#README",
  "name": "vite-plugin-norg",
  "version": "1.0.0",
  "type": "module",
  "description": "A Vite plugin for rendering Norg files",
  "main": "dist/plugin/index.js",
  "types": "dist/plugin/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "wasm-pack build --target web .",
    "build:js": "vite build",
    "clean": "rimraf dist wasm pkg target/wasm32-unknown-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "vite",
    "plugin",
    "norg",
    "neorg",
    "wasm",
    "rust",
    "svelte",
    "react"
  ],
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "20.19.1",
    "@types/react": "19.1.8",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "rimraf": "6.0.1",
    "svelte": "5.35.6",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "vite": "7.0.4",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-top-level-await": "1.5.0",
    "vite-plugin-wasm": "3.5.0",
    "vitest": "3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bottd/vite-plugin-norg.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "shiki": "^3.8.0",
    "zod": "4.0.5"
  }
}
