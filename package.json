{
  "author": "Drake Bott",
  "license": "MIT",
  "homepage": "https://github.com/bottd/vite-plugin-norg#README",
  "name": "vite-plugin-norg",
  "version": "0.1.0",
  "type": "module",
  "description": "A Vite plugin for processing Norg files with WASM-compiled Rust parser",
  "main": "dist/plugin/index.js",
  "types": "dist/plugin/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "wasm-pack build --target web .",
    "build:js": "vite build",
    "clean": "rimraf dist wasm pkg target/wasm32-unknown-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "vite",
    "plugin",
    "norg",
    "neorg",
    "wasm",
    "rust",
    "svelte",
    "react"
  ],
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/node": "^20.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.2.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bottd/vite-plugin-norg.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "zod": "^3.25.67"
  }
}
