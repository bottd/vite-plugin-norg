{
  "author": "Drake Bott",
  "license": "MIT",
  "homepage": "https://github.com/bottd/vite-plugin-norg#README",
  "name": "vite-plugin-norg",
  "version": "1.1.4",
  "type": "module",
  "description": "A Vite plugin for rendering Norg files",
  "main": "dist/plugin/index.js",
  "types": "dist/plugin/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "wasm-pack build --target web .",
    "build:js": "vite build",
    "clean": "rimraf dist wasm pkg target/wasm32-unknown-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "vite",
    "plugin",
    "norg",
    "neorg",
    "wasm",
    "rust",
    "svelte",
    "react"
  ],
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "16.4.0",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
    "react": "^19.1.1",
    "rimraf": "6.0.1",
    "svelte": "^5.38.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "3.5.0",
    "vitest": "3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bottd/vite-plugin-norg.git"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "shiki": "^3.12.2",
    "zod": "^4.1.5"
  }
}
